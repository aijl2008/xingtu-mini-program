/* pages/inform/inform.wxss */
.inform-container{
    width: 100%;
    padding: 17rpx 32rpx 32rpx 32rpx;
}

.label-2 {
    margin-bottom: 15rpx;
}

.label-2__text {
    display: inline-block;
    vertical-align: middle;
}

.label-2__icon {
    position: relative;
    display: inline-block;
    vertical-align: middle;
    margin-right: 10rpx;
    width: 18rpx;
    height: 18rpx;
    background: #fcfff4;
    border-radius: 50rpx;
}

.label-2__icon-checked {
    position: absolute;
    left: 3rpx;
    top: 3rpx;
    width: 12rpx;
    height: 12rpx;
    background: #1aad19;
    border-radius: 50%;
}

.label-4_text {
    text-align: center;
    margin-top: 15rpx;
}