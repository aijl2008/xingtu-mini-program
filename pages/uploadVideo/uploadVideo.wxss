/**index.wxss**/
.container{
    width: 100%;
    padding-top: 160rpx;
}

.form-item-container{
    width: 100%;
    border-bottom: 1rpx solid #EEE;
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding: 20rpx 32rpx;
}
.form-item-container .file_name{
    width: 100%;
    height: 100rpx;
    border: none;
}

.form-item-container .form-header{
    font-size: 32rpx;
    line-height: 1;
    margin: 20rpx 0;
}

.form-item-container .picker{
    font-size: 24rpx;
    line-height: 1;
    color: #CCC;
}
